{
  "name": "mars3d-vue-app",
  "version": "3.6.0",
  "description": "在Vue3技术栈下的Mars3D平台 基础项目",
  "scripts": {
    "dev": "vite --host",
    "serve": "npm run clean-cache && vite --host",
    "clean-cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean-lib": "rimraf node_modules",
    "preview": "vite preview",
    "build": "npm run lint && vite build",
    "build2": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "build:app": "npm run lint && vite build --mode app",
    "serve:dist": "http-server ./dist",
    "lint": "vue-tsc --noEmit --skipLibCheck && npm run eslint",
    "eslint": "eslint ./src/**/*.{js,jsx,vue,ts,tsx} --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@icon-park/svg": "^1.4.1",
    "@icon-park/vue-next": "^1.3.6",
    "@turf/turf": "^6.5.0",
    "ali-oss": "^6.17.1",
    "axios": "^0.25.0",
    "core-js": "^3.20.3",
    "dayjs": "^1.11.0",
    "echarts": "^5.3.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "mars3d": "v3.6.0",
    "mars3d-cesium": "~1.108.0",
    "nprogress": "^0.2.0",
    "qs": "^6.10.3",
    "uuid": "^8.3.2",
    "vant": "4.0.0-alpha.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "consola": "^2.15.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.7.1",
    "http-server": "^14.1.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "postcss-px-to-viewport-8-plugin": "^1.1.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "standard": "^16.0.4",
    "terser": "^5.16.1",
    "typescript": "4.5.5",
    "vconsole": "^3.12.1",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mars3d": "^3.0.3",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^0.40.5"
  },
  "engines": {
   "node": ">=16.0.0",
    "npm": "^7.23.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marsgis/mars3d-vue-project.git"
  },
  "bugs": {
    "url": "https://github.com/marsgis/mars3d-vue-project/issues",
    "email": "wh@marsgis.cn"
  },
  "keywords": [
    "marsgis",
    "mars3d",
    "cesium",
    "vue",
    "vue3",
    "gis",
    "webgis"
  ],
  "author": "火星科技",
  "license": "Apache-2.0",
  "homepage": "http://mars3d.cn",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run eslint"
    ]
  }
}
