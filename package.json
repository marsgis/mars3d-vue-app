{
  "name": "mars3d-vue-app",
  "version": "3.7.0",
  "description": "在Vue3技术栈下的Mars3D平台 手机APP项目",
  "scripts": {
    "dev": "vite --host",
    "serve": "npm run clean-cache && vite --host",
    "clean-cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean-lib": "rimraf node_modules",
    "preview": "vite preview",
    "build": "npm run lint && vite build",
    "build2": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "build:app": "npm run lint && vite build --mode app",
    "serve:dist": "http-server ./dist",
    "lint": "vue-tsc --noEmit --skipLibCheck && npm run eslint",
    "eslint": "eslint ./src/**/*.{js,jsx,vue,ts,tsx} --fix",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@icon-park/svg": "^1.4.2",
    "@icon-park/vue-next": "^1.4.2",
    "@turf/turf": "^6.5.0",
    "@vant/compat": "^1.0.0",
    "ali-oss": "^6.20.0",
    "axios": "^1.6.7",
    "core-js": "^3.36.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "mars3d": "~3.7.0",
    "mars3d-cesium": "~1.119.0",
    "nprogress": "^0.2.0",
    "qs": "^6.11.2",
    "uuid": "^9.0.1",
    "vant": "^4.8.4",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/qs": "^6.9.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.17",
    "consola": "^3.2.3",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.21.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "standard": "^17.1.0",
    "terser": "^5.27.2",
    "typescript": "~5.2.2",
    "unplugin-vue-components": "^0.25.2",
    "vconsole": "^3.15.1",
    "vite": "^5.1.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mars3d": "^4.1.0",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marsgis/mars3d-vue-app.git"
  },
  "bugs": {
    "url": "https://github.com/marsgis/mars3d-vue-app/issues",
    "email": "wh@marsgis.cn"
  },
  "keywords": [
    "marsgis",
    "mars3d",
    "cesium",
    "vue",
    "vue3",
    "gis",
    "webgis"
  ],
  "author": "火星科技",
  "license": "Apache-2.0",
  "homepage": "http://mars3d.cn",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run eslint"
    ]
  }
}
